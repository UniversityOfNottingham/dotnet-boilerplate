{
  "private": true,
  "paths": {
    "src": {
      "sass": "./sass",
      "js": "./js"
    },
    "dest": {
      "root": "./wwwroot/dist",
      "css": "/css",
      "js": "/js"
    }
  },
  "scripts": {
    "precommit": "pretty-quick --staged",
    "clean": "rimraf wwwroot/dist",
    "lint:js": "eslint js/**",
    "lint:sass": "sass-lint sass/**"
  },
  "sasslintConfig": "./node_modules/@universityofnottingham/sasslint-config/.sass-lint.yml",
  "eslintConfig": {
    "extends": "@universityofnottingham"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "browsers": [
              "last 2 versions"
            ]
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@fortawesome/fontawesome-free": "^5.3.1",
    "@universityofnottingham/eslint-config": "^1.0.0",
    "@universityofnottingham/sasslint-config": "^1.0.0",
    "babel-eslint": "^9.0.0",
    "babelify": "^10.0.0",
    "bootstrap": "^4.1.3",
    "browserify": "^16.2.2",
    "del": "^3.0.0",
    "eslint": "^5.5.0",
    "gulp": "^4.0.0",
    "gulp-eslint": "^5.0.0",
    "gulp-header": "^2.0.5",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "^4.0.1",
    "gulp-sass-lint": "^1.4.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify-es": "^1.0.4",
    "gulp-uglifycss": "^1.1.0",
    "husky": "^0.14.3",
    "jquery": "^3.3.1",
    "jquery-validation": "^1.18.0",
    "jquery-validation-unobtrusive": "^3.2.10",
    "popper.js": "^1.14.4",
    "prettier": "^1.14.2",
    "pretty-quick": "^1.6.0",
    "rimraf": "^2.6.2",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  }
}
